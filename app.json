{
  "expo": {
    "name": "PlantaVigia",
    "slug": "detector-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00B894"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },

     "extra": {
      "EXPO_PUBLIC_PLANTNET_API_URL": "${EXPO_PUBLIC_PLANTNET_API_URL}",
      
      "EXPO_PUBLIC_PLANTNET_DISEASES_API_URL": "${EXPO_PUBLIC_PLANTNET_DISEASES_API_URL}",
      
      "EXPO_PUBLIC_KINDUISE_API_URL": "${EXPO_PUBLIC_KINDUISE_API_URL}",
      
      "EXPO_PUBLIC_PLANTNET_API_KEY": "${EXPO_PUBLIC_PLANTNET_API_KEY}",
      "EXPO_PUBLIC_KINDUISE_API_KEY": "${EXPO_PUBLIC_KINDUISE_API_KEY}"
    },
    
    "plugins": [
      "expo-font"
    ]
  }
}
